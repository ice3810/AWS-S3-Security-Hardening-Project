{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowListingBucket",
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket"
            ],
            "Resource": "arn:aws:s3:::my-ice-bucket-1"
        },
        {
            "Sid": "AllowReadingObjects",
            "Effect": "Allow",
            "Action": [
                "s3:GetObject"
            ],
            "Resource": "arn:aws:s3:::my-ice-bucket-1/*"
        }
    ]
}
